{
    "api_token": "TOKEN",
    "chat_id": -1,
    "modules": [
        {
          "name": "auth_module.AuthModule",
          "filter": [
            "egrep \"pam_unix\"",
            "egrep \"session opened\"",
            "egrep \"sshd|su\"",
            "tail -n 10"
          ],
          "schedule": {
            "every": 1,
            "minutes": null
          }
        },
        {
          "name": "mail_module.MailModule",
          "schedule": {
            "at": "00:00",
            "every": 1,
            "day": null
          },
          "thresholds": {
            "received": 0, 
            "delivered": 0, 
            "forwarded": 0, 
            "deferred": 0, 
            "bounced": 0,
            "rejected": 0, 
            "reject_warnings": 0, 
            "held": 0, 
            "discarded": 0,
            "bytes_received": 0, 
            "bytes_delivered": 0, 
            "senders": 0, 
            "sending_hosts_domains": 0,
            "recipients": 0, 
            "recipient_hosts_domains": 0
          },
          "pflogsumm_command": "/usr/local/bin/pflogsumm -d yesterday"
        }
    ],
    "server": "SERVER NAME"
}